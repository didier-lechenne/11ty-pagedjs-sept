{
  "name": "eleventy-dual-output",
  "version": "1.0.0",
  "description": "Configuration Eleventy pour générer web et print avec Paged.js",
  "scripts": {
    "clean": "rimraf .cache _site",
    "build:web": "BUILD_ENV=web npx @11ty/eleventy",
    "build:print": "BUILD_ENV=print npx @11ty/eleventy",
    "build:both": "npm run build:web && npm run build:print",
    "serve:web": "BUILD_ENV=web npx @11ty/eleventy --serve",
    "serve:print": "BUILD_ENV=print npx @11ty/eleventy --serve --port=8081",
    "generate:pdf": "node scripts/generate-pdf.js",
    "dev": "npm run serve:web",
    "preview:print": "npm run build:print && npm run serve:print",
    "production": "npm run build:both && npm run generate:pdf"
  },
"dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-fetch": "^5.0.1",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "cssnano": "^7.0.1",
    "dayjs": "^1.11.5",
    "dotenv-flow": "^4.1.0",
    "esbuild": "^0.25.9",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-eleventy-img": "^0.10.2",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^3.0.0",
    "markdown-it-image-figures": "^2.1.1",
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "rimraf": "^6.0.1",
    "sanitize-html": "^2.7.2",
    "slugify": "^1.6.5"
  },
  "keywords": ["eleventy", "print", "web", "paged-js", "static-site"],
  "author": "Didier Lechenne",
  "license": "MIT"
}